{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "schedule:weekly"
  ],
  "packageRules": [
    {
      "description": "Automatically merge patch-level updates",
      "matchUpdateTypes": ["patch", "digest"],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["^org\\.eclipse\\.jetty:.+"],
      "allowedVersions": "<12.0.0"
    }
  ]
}
